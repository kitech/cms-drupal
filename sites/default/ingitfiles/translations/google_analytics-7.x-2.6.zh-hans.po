msgstr "高级设置"
msgid "Statistics"
msgstr "统计"
msgid "Roles"
msgstr "角色"
msgid "Downloads"
msgstr "下载"
msgid "Users"
msgstr "用户"
msgid "Messages"
msgstr "信息"
msgid "Privacy"
msgstr "隐私"
